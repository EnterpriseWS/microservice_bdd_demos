<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!-- configSection has to be at the FIRST configuration tab -->
  <configSections>
    <section name="specFlow" type="TechTalk.SpecFlow.Configuration.ConfigurationSectionHandler, TechTalk.SpecFlow" />
    <sectionGroup name="wellknownSite">
      <section name="sites"
               type="System.Configuration.NameValueSectionHandler" />
    </sectionGroup>
  </configSections>
  <specFlow>
    <!-- For additional details on SpecFlow configuration options see http://go.specflow.org/doc-config -->
    <unitTestProvider name="MsTest" />
    <generator allowDebugGeneratedFiles="true" />
  </specFlow>
  <wellknownSite>
    <sites>
      <add key="abc" value="https://www.google.com" />
      <add key="def" value="https://www.yahoo.com" />
      <add key="xyz" value="https://www.bing.com" />
    </sites>
  </wellknownSite>
  <appSettings>
    <!-- Supported values: nunit, xunit, mstest, mspec, mbunit and gallio -->
    <add key="FluentAssertions.TestFramework" value="mstest" />
  </appSettings>  <runtime>
